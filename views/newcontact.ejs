</head>
    <body>
      
      <h1>Add contacts here</h1>

        <form  action="/newcontact" method="post">
          <form>
            <div class="form-group">
              <label>contactname</label>
              <input type="text" class="form-control" name="contactname" placeholder="Contact Name" required="">
            </div>
            <div class="form-group">
              <label>contactnumber</label>
              <input type="number" class="form-control" name="contactnumber" placeholder="Contact Number" required="">
            </div>
            <button type="submit" class="btn btn-success">Save</button>
        </form>
</div>
    </body>
